<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Database Content</title>
    <style>
        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: left;
        }
        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <h1>Dashboard</h1>
    <table>
        <thead>
            <tr>
                <th>Name of the Organisation</th>
                <th>Name of the Chairperson</th>
                <th>Number of Employees</th>
                <th>Yearly Turnovers</th>
                <th>Number of Branches</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(item => { %>
                <tr>
                    <td><%= item.orgName %></td>
                    <td><%= item.chairpersonName %></td>
                    <td><%= item.numEmployees %></td>
                    <td><%= item.yearlyTurnover %></td>
                    <td><%= item. numBranches %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
